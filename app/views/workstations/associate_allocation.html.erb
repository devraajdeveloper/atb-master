<table id="dtHorizontalExample" class="table table-striped table-bordered table-sm" cellspacing="0"
  width="100%">
  <thead>
    <tr>
<tr class = "header_row"> <td colspan = "9"> <%= "Master Data : #{session[:associate_name]}" %> </td> </tr>
<th>Encounter No</th><th>Patient Name</th><th>Admit Date</th><th>Discharge Date</th><th>Billed Amount</th>
<th>Balance Amount</th><th>Insurance Name</th><th>Allocation</th><th>Associate ID</th>
</tr>
 <tbody>
<% @master_atb.each do |master_atb| %>
    <tr>
       <td><%= master_atb.encounter_no %></td>
       <td><%= master_atb.patient_name %></td>
       <td><%= master_atb.admit_date %></td>
       <td><%= master_atb.discharge_date %></td>
       <td><%= master_atb.billed_amount %></td>
       <td><%= master_atb.balance_amount %></td>
       <td><%= master_atb.insurance_name %></td>
       <td><%= master_atb.user_allocation %></td>
       <td><%= master_atb.associate_id %></td>
    </tr>
<% end %>
 </tbody>
</table>