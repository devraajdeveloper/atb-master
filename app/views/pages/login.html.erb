<%= form_for @associate do |f| %>
<div class = "container set-margin-login">
<div class = "container ">
<div class = jumbotron>
  <div class="form-group">
    <%= f.label :"Associate ID" %>
    <%= f.text_field :"associate_id", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :"Password" %>
    <%= f.password_field :"password", class: "form-control" %>
  </div>
    <%= f.submit :"Login", class: "btn btn-primary" %>
    </div>
    </div>
<% end %>
