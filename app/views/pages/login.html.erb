<%= form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: login_path) do |f| %>
<div class = "container set-margin-login">
<div class = "container ">
<div class = "jumbotron">
  <h3 class="center"> Enter Associate ID and Password </h3>
  <div class="form-group">
    <%= f.label :"Associate ID" %>
    <%= f.text_field :"associate_id", class: "form-control center" %>
  </div>
  <div class="form-group">
    <%= f.label :"Password" %>
    <%= f.password_field :"password", class: "form-control center" %>
  </div>
    <%= f.submit :"Login", class: "btn btn-primary" %>
    </div>
    </div>
<% end %>
