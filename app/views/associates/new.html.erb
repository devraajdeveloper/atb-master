<%= form_for @associate do |f| %>
<div class = "container">
<div class = "jumbotron">
<div class="form-group">
<%= f.label :"Associate ID" %>
<%= f.text_field :"associate_id", class: "form-control", :required => true %>
</div>
<div class="form-group">
<%= f.label :"Associate Name" %>
<%= f.text_field :"associate_name", class: "form-control", :required => true %>
</div>
<div class="form-group">
<%= f.label :"Project Name" %>
<%= f.text_field :"project_name", class: "form-control", :required => true %>
</div>
<div class="form-group">
<%= f.label :"Password" %>
<%= f.text_field :"password", class: "form-control" %>
</div>
<%= f.submit :"Add User", class: "btn btn-primary" %>
</div>
</div>
<% end %>